{
 "name": "mainengine",
 "version": "1.0.10",
 "private": true,
 "description": "A toolkit for building Expo apps.",
 "workspaces": [
  "apps/*",
  "packages/*"
 ],
 "scripts": {
  "start": "cd apps/example && yarn start",
  "prepack": "cd packages/backpack && yarn prepack",
  "build": "cd packages/backpack && yarn build",
  "release": "release-it",
  "test": "jest",
  "typecheck": "tsc --noEmit",
  "lint": "eslint \"**/*.{js,ts,tsx}\""
 },
 "release-it": {
  "plugins": {
   "@release-it-plugins/workspaces": {
    "workspaces": [
     "packages/*"
    ]
   }
  },
  "npm": false
 },
 "devDependencies": {
  "@babel/core": "^7.19.3",
  "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
  "@commitlint/config-conventional": "^17.0.2",
  "@evilmartians/lefthook": "^1.2.2",
  "@expo/config": "~8.0.0",
  "@expo/webpack-config": "^18.0.1",
  "@react-native-community/eslint-config": "^3.0.2",
  "@release-it-plugins/workspaces": "^3.2.0",
  "@release-it/conventional-changelog": "^5.0.0",
  "@trivago/prettier-plugin-sort-imports": "^3.2.0",
  "@types/jest": "^27.4.0",
  "@types/mapbox-gl": "^2.6.3",
  "@types/react": "~18.0.24",
  "@types/react-native": "0.70.0",
  "@types/react-test-renderer": "^17.0.1",
  "@types/seedrandom": "^3.0.2",
  "@types/styled-components": "^5.1.22",
  "@types/styled-components-react-native": "^5.1.3",
  "@typescript-eslint/eslint-plugin": "5.10.2",
  "@typescript-eslint/parser": "^5.19.0",
  "babel-loader": "^8.2.4",
  "babel-plugin-module-resolver": "^4.1.0",
  "babel-plugin-styled-components": "^2.0.7",
  "commitlint": "^17.0.2",
  "del-cli": "^5.0.0",
  "eslint": "^8.8.0",
  "eslint-config-airbnb-typescript": "12.0.0",
  "eslint-config-prettier": "6.15.0",
  "eslint-config-react-app": "6.0.0",
  "eslint-import-resolver-typescript": "^2.5.0",
  "eslint-plugin-import": "^2.25.4",
  "eslint-plugin-jsx-a11y": "^6.5.1",
  "eslint-plugin-prettier": "3.1.4",
  "eslint-plugin-react": "^7.28.0",
  "eslint-plugin-react-hooks": "^4.3.0",
  "eslint-plugin-react-native": "^4.0.0",
  "eslint-plugin-storybook": "^0.5.6",
  "eslint-plugin-unused-imports": "^2.0.0",
  "husky": "^8.0.0",
  "jest": "^29.2.1",
  "lint-staged": "12.3.3",
  "pod-install": "^0.1.0",
  "prettier": "2.5.1",
  "prettier-plugin-sort-imports": "^1.6.0",
  "react": "18.2.0",
  "react-native": "0.71.8",
  "react-native-builder-bob": "^0.20.4",
  "release-it": "^15.0.0",
  "typescript": "^4.6.3"
 }
}
